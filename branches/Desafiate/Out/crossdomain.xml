<?xml version="1.0"?>
<!DOCTYPE cross-domain-policy SYSTEM
"http://www.adobe.com/xml/dtds/cross-domain-policy.dtd">

<cross-domain-policy>
	<site-control permitted-cross-domain-policies="master-only"/>
	<allow-access-from domain="desafiate.unusualwonder.com"/>
	<allow-access-from domain="*.desafiate.es"/>
	<allow-http-request-headers-from domain="desafiate.unusualwonder.com" headers="SOAPAction"/>
	<allow-http-request-headers-from domain="*.desafiate.es" headers="SOAPAction"/>
</cross-domain-policy>